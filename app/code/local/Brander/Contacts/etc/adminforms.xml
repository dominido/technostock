<?xml version="1.0"?>
<adminforms>
    <entities>
		<shops_list>
			<attributes>
				<page_id>
					<template>page_id</template>
				</page_id>
				<title>
					<template>text</template>
					<required>1</required>
				</title>
                <adress>
                    <template>text</template>
                    <required>0</required>
                </adress>
                <phone>
                    <template>text</template>
                    <required>0</required>
                </phone>
                <image>
                    <template>image</template>
                </image>
                <longitude>
                    <template>text</template>
                    <required>1</required>
                </longitude>
                <latitude>
                    <template>text</template>
                    <required>1</required>
                </latitude>
				<position>
					<type>int</type>
					<template>text</template>
                    <required>1</required>
				</position>
				<status>
					<type>int</type>
                    <template>status</template>
				</status>
			</attributes>
			<grid>
				<fields>
					<title/>
					<adress/>
					<image>
                        <image_width>150</image_width>
                    </image>
					<position/>
					<status/>
				</fields>
			</grid>
		</shops_list>
    </entities>
</adminforms>
