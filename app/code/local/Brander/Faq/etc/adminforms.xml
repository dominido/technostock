<?xml version="1.0"?>
<adminforms>
    <entities>
		<branderfaqentities>
			<attributes>
				<page_id>
					<template>page_id</template>
				</page_id>
				<customer_name>
					<template>text</template>
					<required>1</required>
				</customer_name>
				<customer_email>
					<template>text</template>
					<required>1</required>
				</customer_email>
				<question>
					<template>textarea</template>
					<required>1</required>
				</question>
				<answer>
					<template>textarea</template>
					<required>1</required>
				</answer>
				<created_at>
					<template>datetime</template>
					<required>1</required>
				</created_at>
				<position>
					<type>int</type>
					<template>text</template>
				</position>
				<status>
					<type>int</type>
					<template>dropdown</template>
					<source>brander_faq/system_config_source_status</source>
				</status>
			</attributes>
			<grid>
				<fields>
					<customer_name/>
					<customer_email/>
					<question/>
					<answer/>
					<position/>
					<status/>
				</fields>
			</grid>
		</branderfaqentities>
    </entities>
</adminforms>
